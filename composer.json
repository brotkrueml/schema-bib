{
	"name": "brotkrueml/schema-bib",
	"description": "Extend the schema extension with bibliographic-related terms",
	"license": "GPL-2.0-or-later",
	"type": "typo3-cms-extension",
	"keywords": [
		"TYPO3 CMS",
		"TYPO3 extension",
		"JSON-LD",
		"Structured data",
		"SEO",
		"schema.org"
	],
	"authors": [
		{
			"name": "Chris MÃ¼ller",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/brotkrueml/schema-bib/issues",
		"forum": "https://github.com/brotkrueml/schema/discussions",
		"source": "https://github.com/brotkrueml/schema-bib",
		"docs": "https://docs.typo3.org/p/brotkrueml/schema-bib/main/en-us/"
	},
	"require": {
		"php": ">=8.1",
		"brotkrueml/schema": "^3.0@dev",
		"typo3/cms-core": "^11.5.19 || ^12.4"
	},
	"require-dev": {
		"brotkrueml/coding-standards": "~5.0.0",
		"ergebnis/composer-normalize": "~2.37.0"
	},
	"autoload": {
		"psr-4": {
			"Brotkrueml\\SchemaBib\\": "Classes/"
		}
	},
	"config": {
		"allow-plugins": {
			"ergebnis/composer-normalize": true,
			"typo3/class-alias-loader": true,
			"typo3/cms-composer-installers": true
		},
		"bin-dir": ".Build/bin",
		"sort-packages": true,
		"vendor-dir": ".Build/vendor"
	},
	"extra": {
		"branch-alias": {
			"dev-main": "2.0.x-dev"
		},
		"typo3/cms": {
			"app-dir": ".Build",
			"extension-key": "schema_bib",
			"web-dir": ".Build/web"
		}
	}
}
